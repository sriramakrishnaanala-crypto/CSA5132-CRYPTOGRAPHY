#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char plain[] = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
    char keyword[] = "CIPHER";
    char cipher[27] = "";
    char text[100];
    int used[26] = {0};
    int i, k = 0;

    // Add keyword letters
    for(i = 0; keyword[i] != '\0'; i++) {
        char ch = toupper(keyword[i]);
        if(!used[ch - 'A']) {
            cipher[k++] = ch;
            used[ch - 'A'] = 1;
        }
    }

    // Add remaining letters
    for(i = 0; i < 26; i++) {
        if(!used[i]) {
            cipher[k++] = 'A' + i;
        }
    }

    cipher[k] = '\0';

    printf("Cipher Alphabet
