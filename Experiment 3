#include <stdio.h>
#include <string.h>
#include <ctype.h>

char matrix[5][5];

// Function to create matrix
void createMatrix(char key[]) {
    int used[26] = {0};
    int i, j, k = 0;
    char ch;

    used['J' - 'A'] = 1;  // I and J treated same

    for(i = 0; key[i] != '\0'; i++) {
        ch = toupper(key[i]);
        if(ch >= 'A' && ch <= 'Z' && !used[ch - 'A']) {
            matrix[k/5][k%5] = ch;
            used[ch - 'A'] = 1;
            k++;
        }
    }

    for(i = 0; i < 26; i++) {
        if(!used[i]) {
            matrix[k/5]
